{
    "name": "nexendrie/site-generator",
    "description": "Static site generator which turns Markdown into HTML",
    "keywords": [
        "static", "site", "website", "generator", "markdown", "html"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/site-generator/",
        "issues": "https://gitlab.com/nexendrie/site-generator/issues",
        "docs": "https://nexendrie.gitlab.io/site-generator"
    },
    "require": {
        "php": ">=7.1.0",
        "cebe/markdown": "^1.1",
        "nette/finder": "^2.4",
        "nette/neon": "^2.4",
        "nette/utils": "^2.4",
        "nette/command-line": "^1.1"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nette/tester": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\SiteGenerator\\": "src/"
        }
    },
    "bin": [
        "src/generate-site"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
